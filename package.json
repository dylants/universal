{
  "name": "universal",
  "description": "Universal App",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.production.js --progress --profile --colors --optimize-minimize",
    "build:server": "./node_modules/.bin/babel ./app --out-dir ./build --source-maps --copy-files",
    "build": "npm run lint && npm run clean:build && npm run build:server && npm run build:client",
    "clean:build": "rimraf build && rimraf public",
    "clean:coverage": "rimraf coverage",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:coverage && npm run clean:node_modules",
    "lint:eslint": "eslint .",
    "lint:sass-lint": "sass-lint -c ./.sass-lint.yml './app/**/*.scss' -v",
    "lint": "npm run lint:eslint && npm run lint:sass-lint",
    "production-build": "npm run build && npm run clean:node_modules && yarn install --production",
    "production": "NODE_ENV=production node ./build/config/server.js",
    "reinstall": "npm run clean && npm install",
    "start": "./node_modules/.bin/nodemon --exec NODE_ENV=development ./node_modules/.bin/babel-node ./app/config/server.js",
    "test:coverage": "NODE_ENV=test ./node_modules/.bin/jest --config .jestrc.json --coverage",
    "test:jest": "NODE_ENV=test ./node_modules/.bin/jest --config .jestrc.json --verbose",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/jest --config .jestrc.json --watch",
    "test": "npm run lint && npm run test:jest"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "body-parser": "1.17.2",
    "compression": "1.6.2",
    "consolidate": "0.14.5",
    "css-modules-require-hook": "4.0.6",
    "debug-caller": "2.2.0",
    "express": "4.15.3",
    "handlebars": "4.0.10",
    "history": "4.6.2",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "node-sass": "4.5.3",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-config": "1.0.0-beta.3",
    "react-router-dom": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "jsdom": "10.1.0",
    "jsdom-global": "3.0.2",
    "lodash-webpack-plugin": "0.11.4",
    "nodemon": "1.11.0",
    "normalize.css": "7.0.0",
    "react-hot-loader": "3.0.0-beta.0",
    "react-test-renderer": "15.5.4",
    "redux-mock-store": "1.2.3",
    "rimraf": "2.6.1",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  }
}
