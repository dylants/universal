{
  "name": "universal",
  "description": "Universal App",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "./node_modules/.bin/babel -d ./build ./server -s",
    "build": "npm run lint && npm run clean:build && npm run build:server && npm run build:client",
    "clean:build": "rimraf build && rimraf public",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "lint:eslint": "eslint .",
    "lint:sass-lint": "sass-lint -c ./.sass-lint.yml './app/**/*.scss' -v",
    "lint": "npm run lint:eslint && npm run lint:sass-lint",
    "production-build": "npm run build && npm run clean:node_modules && npm install --production",
    "production": "NODE_ENV=production node ./build/server.js",
    "reinstall": "npm run clean && npm install",
    "start": "./node_modules/.bin/nodemon --exec NODE_ENV=development ./node_modules/.bin/babel-node ./app/server.js",
    "test:mocha": "NODE_ENV=test ./node_modules/.bin/mocha --require ./test/test_setup.js --compilers js:babel-core/register --recursive test/**/*.test.js",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/mocha --require ./test/test_setup.js --compilers js:babel-core/register --recursive --watch test/**/*.test.js",
    "test": "npm run lint && npm run test:mocha"
  },
  "dependencies": {
    "consolidate": "0.14.5",
    "debug-caller": "2.2.0",
    "express": "4.15.2",
    "handlebars": "4.0.6",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-plugin-transform-strict-mode": "6.22.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-airbnb-base": "11.1.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "mocha": "3.2.0",
    "node-sass": "4.5.1",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "sass-lint": "1.10.2",
    "should": "11.2.1"
  }
}
